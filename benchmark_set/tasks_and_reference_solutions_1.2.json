        {
            "task_ID": "TASK_1_2_033",
            "task_text": "To study the diffusion direction of parks in Wuhan, please extract the centroid of each park and conduct a directional analysis on these centroids.",
            "task_labels": [
                "Spatial operations",
                "Task Set 03",
                "Process, Merge, Visualize",
                "Merge, Visualize",
                "Heatmaps, Contour Lines"
            ],
            "reference_solution_description": "Load the Wuhan park polygons, derive a centroid for each feature, and run the standard deviational ellipse analysis on those centroids to summarize the primary diffusion direction.",
            "reference_solutions": [
                {
                    "steps": [
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "Wuhan Urban Parks",
                                "output_geodataframe_name": "wuhan_parks"
                            },
                            "comment": "Bring the Wuhan Urban Parks polygon dataset into memory so the geometry for every park can be accessed for centroid extraction."
                        },
                        {
                            "function_name": "get_centroids",
                            "arguments": {
                                "geodataframe_name": "wuhan_parks",
                                "output_geodataframe_name": "wuhan_parks_centroids"
                            },
                            "comment": "Convert each park polygon to its centroid point, producing the point feature set required for directional distribution analysis."
                        },
                        {
                            "function_name": "calculate_standard_directional_ellipse",
                            "arguments": {
                                "geodataframe_name": "wuhan_parks_centroids",
                                "output_geodataframe_name": "wuhan_parks_directional_ellipse",
                                "output_variable_name": "wuhan_parks_directional_ellipse_stats"
                            },
                            "comment": "Compute the standard deviational ellipse for the park centroids to capture the major/minor axes and dominant orientation representing the parks' diffusion trend."
                        }
                    ]
                }
            ]
        },
        {
            "task_ID": "TASK_1_2_034",
            "task_text": "Load the data for the source region of the Yellow River and the Jiuzhaigou earthquake zone, compare the two vector extents of these regions, and calculate the percentage of their overlapping area relative to their respective total areas.",
            "task_labels": [
                "Spatial operations",
                "Task Set 03",
                "Process, Merge, Visualize",
                "Merge, Visualize",
                "Heatmaps, Contour Lines"
            ],
            "reference_solution_description": "Load the Yellow River source-region and Jiuzhaigou earthquake-zone polygons, compute their areas, intersect the two extents, measure the overlap, and divide the overlap area by each region's total area to report the percentage coverage for both datasets.",
            "reference_solutions": [
                {
                    "steps": [
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "The source region of the Yellow River",
                                "output_geodataframe_name": "yellow_river_region"
                            },
                            "comment": "Load the Yellow River source region polygons so their extent is ready for measurement and overlap analysis."
                        },
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "Jiuzhaigou earthquake zone",
                                "output_geodataframe_name": "jiuzhaigou_earthquake_zone"
                            },
                            "comment": "Load the Jiuzhaigou earthquake zone extent so it can be compared against the Yellow River region."
                        },
                        {
                            "function_name": "calculate_polygon_areas",
                            "arguments": {
                                "geodataframe_name": "yellow_river_region",
                                "output_variable_name": "yellow_river_area_stats",
                                "area_column_name": "yellow_river_area_sq_km"
                            },
                            "comment": "Project and measure the Yellow River source region to obtain both per-feature areas and the total area value."
                        },
                        {
                            "function_name": "calculate_polygon_areas",
                            "arguments": {
                                "geodataframe_name": "jiuzhaigou_earthquake_zone",
                                "output_variable_name": "jiuzhaigou_area_stats",
                                "area_column_name": "jiuzhaigou_area_sq_km"
                            },
                            "comment": "Repeat the area calculation for the Jiuzhaigou earthquake zone so its total footprint is known."
                        },
                        {
                            "function_name": "analyze_vector_overlap",
                            "arguments": {
                                "geodataframe_a_name": "yellow_river_region",
                                "geodataframe_b_name": "jiuzhaigou_earthquake_zone",
                                "output_geodataframe_name": "overlap_area"
                            },
                            "comment": "Intersect the two regions to isolate their shared footprint."
                        },
                        {
                            "function_name": "calculate_polygon_areas",
                            "arguments": {
                                "geodataframe_name": "overlap_area",
                                "output_variable_name": "overlap_area_stats",
                                "area_column_name": "overlap_area_sq_km"
                            },
                            "comment": "Measure the overlap polygon so the shared area value is available for percentage calculations."
                        }
                    ]
                }
            ]
        },
        {
            "task_ID": "TASK_1_2_035",
            "task_text": "Calculate the total area of China's forest water conservation zones within a 10000 meter buffer zone of the Loess Plateau Region, and compute the percentage it represents of the entire buffer zone area.",
            "task_labels": [
                "Spatial operations",
                "Task Set 03",
                "Process, Merge, Visualize",
                "Merge, Visualize",
                "Heatmaps, Contour Lines"
            ],
            "reference_solution_description": "Load the Loess Plateau Region and Forest Water Conservation China datasets, build a 10 km dissolved buffer around the plateau, intersect the buffer with the conservation polygons, measure both the buffer and the overlap areas, and divide the overlap by the full buffer area to report the percentage of conservation land inside the buffer.",
            "reference_solutions": [
                {
                    "steps": [
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "Loess Plateau Region",
                                "output_geodataframe_name": "loess_plateau"
                            },
                            "comment": "Load the Loess Plateau Region boundaries to serve as the base geometry for buffering."
                        },
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "Forest Water Conservation China",
                                "output_geodataframe_name": "forest_water_conservation"
                            },
                            "comment": "Bring the nationwide forest water conservation zones into memory so they can be intersected with the buffered plateau."
                        },
                        {
                            "function_name": "create_dissolved_buffer",
                            "arguments": {
                                "geodataframe_name": "loess_plateau",
                                "buffer_distance_meters": 10000,
                                "output_geodataframe_name": "loess_plateau_buffer"
                            },
                            "comment": "Generate a dissolved 10,000-meter buffer around the Loess Plateau to capture the surrounding area of interest."
                        },
                        {
                            "function_name": "analyze_vector_overlap",
                            "arguments": {
                                "geodataframe_a_name": "forest_water_conservation",
                                "geodataframe_b_name": "loess_plateau_buffer",
                                "output_geodataframe_name": "overlap_forest_water_conservation"
                            },
                            "comment": "Intersect the conservation zones with the plateau buffer to isolate only the forest areas that fall within the 10 km zone."
                        },
                        {
                            "function_name": "calculate_polygon_areas",
                            "arguments": {
                                "geodataframe_name": "loess_plateau_buffer",
                                "output_variable_name": "buffer_area_stats",
                                "area_column_name": "buffer_area_sq_km"
                            },
                            "comment": "Measure the total area of the dissolved buffer so its size in square kilometers is known."
                        },
                        {
                            "function_name": "calculate_polygon_areas",
                            "arguments": {
                                "geodataframe_name": "overlap_forest_water_conservation",
                                "output_variable_name": "forest_water_conservation_area_stats",
                                "area_column_name": "forest_area_sq_km"
                            },
                            "comment": "Measure the buffered-overlap layer to quantify the conservation area contained within the 10 km zone."
                        },
                        {
                            "function_name": "perform_arithmetic_operation",
                            "arguments": {
                                "values": [
                                    "state:forest_water_conservation_area_stats.total_area_sq_km",
                                    "state:buffer_area_stats.total_area_sq_km"
                                ],
                                "operation": "divide",
                                "output_variable_name": "percentage_forest_water_conservation"
                            },
                            "comment": "Divide conservation overlap area by total buffer area to get the share of the buffer occupied by forest water conservation zones (multiply by 100 for percent if required)."
                        }
                    ]
                }
            ]
        },
        {
            "task_ID": "TASK_1_2_036",
