                    ]
                }
            ]
        },
        {
            "task_ID": "TASK_1_2_037",
            "task_text": "Identify all areas within the Weihe River Basin where the slope is greater than 5 and the distance to the nearest traditional village is less than 20 kilometers.",
            "task_labels": [
                "Spatial operations",
                "Task Set 03",
                "Process, Merge, Visualize",
                "Merge, Visualize",
                "Heatmaps, Contour Lines"
            ],
            "reference_solution_description": "Retrieve the Weihe Basin slope raster, delineate its basin boundary, filter China's traditional villages that fall inside the basin, build a dissolved 20 km buffer around those villages, reclassify the slope raster to isolate values greater than five, rasterize the village buffer to match the slope grid, and analyze the overlap of the two rasters to highlight pixels that satisfy both criteria.",
            "reference_solutions": [
                {
                    "steps": [
                        {
                            "function_name": "get_raster_path",
                            "arguments": {
                                "rasterdataset": "Wei River Basin Topographic Slope Classification Dataset"
                            },
                            "comment": "Fetch the catalog path for the Weihe Basin slope classification raster so it can be used in subsequent boundary extraction and reclassification."
                        },
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "The source region of the Yellow River",
                                "output_geodataframe_name": "weihe_river_basin"
                            },
                            "comment": "Load the Weihe River basin polygons to provide a reference area for filtering villages and contextualizing the raster results."
                        },
                        {
                            "function_name": "extract_raster_boundary",
                            "arguments": {
                                "raster_path": "d:/code/GeoBenchX/GeoBenchX/data/Data/GeoData/WeiheBasin.tif",
                                "output_geodataframe_name": "weihe_river_basin_boundary"
                            },
                            "comment": "Convert the valid extent of the Weihe Basin raster into a polygon boundary so spatial selections can be tied precisely to the raster footprint."
                        },
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "Traditional Villages of China",
                                "output_geodataframe_name": "traditional_villages"
                            },
                            "comment": "Bring in the nationwide traditional villages dataset to identify settlements that lie inside the Weihe Basin."
                        },
                        {
                            "function_name": "select_features_by_spatial_relationship",
                            "arguments": {
                                "features_geodataframe_name": "traditional_villages",
                                "reference_geodataframe_name": "weihe_river_basin_boundary",
                                "spatial_predicates": [
                                    "within"
                                ],
                                "output_geodataframe_name": "villages_within_weihe",
                                "plot_result": false
                            },
                            "comment": "Filter the traditional villages to only those located within the raster-derived basin boundary."
                        },
                        {
                            "function_name": "create_dissolved_buffer",
                            "arguments": {
                                "geodataframe_name": "villages_within_weihe",
                                "buffer_size_meters": 20000,
                                "output_geodataframe_name": "villages_buffer_20km",
                                "plot_title": "20 km Buffer around Traditional Villages in Weihe River Basin"
                            },
                            "comment": "Create a dissolved 20 km buffer around all qualifying villages to capture locations within 20 kilometers of a traditional village."
                        },
                        {
                            "function_name": "classify_raster_zones",
                            "arguments": {
                                "raster_path": "d:/code/GeoBenchX/GeoBenchX/data/Data/GeoData/WeiheBasin.tif",
                                "reclassification_table": "[{\"min_value\": 5.1, \"max_value\": 100, \"new_value\": 1, \"label\": \"Slope > 5\"}]",
                                "output_variable_name": "classified_slope",
                                "output_raster_path": "d:/code/GeoBenchX/GeoBenchX/scratch/classified_slope.tif",
                                "plot_title": "Reclassified Slope Raster for Weihe River Basin",
                                "append_timestamp_to_output": false
                            },
                            "comment": "Reclassify the slope raster so pixels with slope greater than five are flagged with a value of 1 for downstream overlap analysis."
                        },
                        {
                            "function_name": "rasterize_vector_to_match_raster",
                            "arguments": {
                                "geodataframe_name": "villages_buffer_20km",
                                "reference_raster_path": "d:/code/GeoBenchX/GeoBenchX/data/Data/GeoData/WeiheBasin.tif",
                                "output_variable_name": "rasterized_buffer",
                                "output_raster_path": "d:/code/GeoBenchX/GeoBenchX/scratch/rasterized_buffer.tif",
                                "append_timestamp_to_output": false
                            },
                            "comment": "Rasterize the dissolved 20 km buffer so it shares the same grid resolution and extent as the slope raster."
                        },
                        {
                            "function_name": "analyze_raster_overlap",
                            "arguments": {
                                "raster1_path": "d:/code/GeoBenchX/GeoBenchX/scratch/classified_slope.tif",
                                "raster2_path": "d:/code/GeoBenchX/GeoBenchX/scratch/rasterized_buffer.tif",
                                "output_variable_name": "final_result",
                                "plot_result": true
                            },
                            "comment": "Overlay the slope>5 raster with the village-distance raster to isolate and report areas where both criteria are satisfied."
                        }
                    ]
                }
            ]
        },
        {
