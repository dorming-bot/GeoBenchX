            "task_ID": "TASK_251113_142843_001001",
            "task_text": "Count the total number of POI points in the 2014 China POI data that fall within Chengdu City.",
            "task_labels": [
                "Spatial operations",
                "Task Set 03",
                "Process, Merge, Visualize",
                "Merge, Visualize",
                "Heatmaps, Contour Lines"
            ],
            "reference_solution_description": "Load the Chengdu municipal polygon and the 2014 POI point shapefile from data/Data/GeoData, select the POIs located within the Chengdu boundary, and tally the resulting features to report the total count.",
            "reference_solutions": [
                {
                    "steps": [
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "2014 China POI",
                                "output_geodataframe_name": "china_poi_2014"
                            },
                            "comment": "Register '2014 China POI': 'data/Data/GeoData/2014_China_POI.shp' in GEO_CATALOG so this call loads all POI points into GeoDataFrame 'china_poi_2014'."
                        },
                        {
                            "function_name": "load_geodata",
                            "arguments": {
                                "geodataset": "Chengdu Surface",
                                "output_geodataframe_name": "chengdu_boundary"
                            },
                            "comment": "Register 'Chengdu Surface': 'data/Data/GeoData/Chengdu_Surface.shp' in GEO_CATALOG to bring the Chengdu city polygon into GeoDataFrame 'chengdu_boundary'."
                        },
                        {
                            "function_name": "select_features_by_spatial_relationship",
                            "arguments": {
                                "features_geodataframe_name": "china_poi_2014",
                                "reference_geodataframe_name": "chengdu_boundary",
                                "spatial_predicates": [
                                    "within"
                                ],
                                "output_geodataframe_name": "poi_within_chengdu",
                                "plot_result": false
                            },
                            "comment": "Filter POI points that fall inside Chengdu's polygon; the returned message reports how many features satisfy the 'within' predicate and stores them in 'poi_within_chengdu'."
                        },
                        {
                            "function_name": "calculate_column_statistics",
                            "arguments": {
                                "dataframe_name": "poi_within_chengdu",
                                "column_name": "OBJECTID",
                                "output_variable_name": "poi_within_chengdu_stats",
                                "include_quantiles": false,
                                "additional_quantiles": null
                            },
                            "comment": "Use any stable numeric identifier column (e.g., OBJECTID or FID) to get the exact record count for POIs inside Chengdu; adjust column_name if the shapefile uses a different field."
                        }
                    ]
                }
            ],
